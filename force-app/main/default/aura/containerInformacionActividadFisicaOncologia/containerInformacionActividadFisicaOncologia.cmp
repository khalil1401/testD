<aura:component implements="forceCommunity:availableForAllPageTypes,flexipage:availableForAllPageTypes">

    <aura:attribute name="titulo" type="String"/>
    <aura:attribute name="cuerpoTexto" type="String"/>

    <div    class="containerInformacionAFOncologia">
        <div class="slds-grid">
            <div class="slds-col slds-large-size_1-of-12">
            </div>
            <div class="slds-col slds-size_1-of-1 slds-large-size_10-of-12">
                <div class="slds-m-top_x-large">
                    <aura:unescapedHtml value="{!v.titulo}" />
                </div>
                <div class="slds-m-bottom_x-large">
                    <aura:unescapedHtml value="{!v.cuerpoTexto}" />
                </div>
            </div>
            <div class="slds-col slds-large-size_1-of-12">
            </div>
        </div>
    </div>

</aura:component>