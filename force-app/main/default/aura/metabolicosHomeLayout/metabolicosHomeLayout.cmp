<aura:component implements="forceCommunity:layout">

    <aura:attribute name="container1" type="Aura.Component[]" required="false"/>
    <aura:attribute name="container2" type="Aura.Component[]" required="false"/>
    <aura:attribute name="container3" type="Aura.Component[]" required="false"/>
    <aura:attribute name="container4" type="Aura.Component[]" required="false"/>
    <aura:attribute name="container5" type="Aura.Component[]" required="false"/>
    <aura:attribute name="container6" type="Aura.Component[]" required="false"/>
    <aura:attribute name="container7" type="Aura.Component[]" required="false"/>
    <aura:attribute name="container8" type="Aura.Component[]" required="false"/>
    <aura:attribute name="container9" type="Aura.Component[]" required="false"/>
    <aura:attribute name="container10" type="Aura.Component[]" required="false"/>
    <aura:attribute name="container11" type="Aura.Component[]" required="false"/>

    <div class="slds-col slds-size_1-of-1">
        {!v.container1}
    </div>

    <div class="slds-col slds-size_1-of-1">
        {!v.container11}
    </div>

    <div class="slds-col slds-size_1-of-1">
        {!v.container2}
    </div>

    <div class="slds-col slds-size_1-of-1 slds-m-top_x-large">
        {!v.container3}
    </div>

    <div class="slds-col slds-size_1-of-1 slds-m-top_xx-large">
        <div class="div-mhl-meta">
            {!v.container4}
        </div>
    </div>

    <div class="slds-col slds-size_1-of-1 slds-m-top_xx-large">
        {!v.container5}
    </div>

    <div class="slds-grid slds-wrap">
        <div class="slds-col slds-medium-size_1-of-12">
        </div>
        <div class="slds-col slds-size_1-of-1 slds-medium-size_10-of-12">
            <div class="slds-grid slds-wrap slds-grid_align-center">
                <div class="slds-col slds-size_1-of-1 slds-medium-size_4-of-12 slds-text-align_center div-card-home-left-meta slds-m-top_x-large">
                    {!v.container6}
                </div>
                <div class="slds-col slds-size_1-of-1 slds-medium-size_4-of-12 slds-text-align_center div-card-home-center-meta slds-m-top_x-large">
                    {!v.container7}
                </div>
                <div class="slds-col slds-size_1-of-1 slds-medium-size_4-of-12 slds-text-align_center div-card-home-right-meta slds-m-top_x-large">
                    {!v.container8} 
                </div>
            </div>
        </div>
        <div class="slds-col slds-medium-size_1-of-12">
        </div>
    </div>

    <div class="slds-col slds-size_1-of-1 slds-m-top_x-large slds-m-bottom_x-large" id="bki">
        {!v.container9}
    </div>

    <div class="slds-col slds-size_1-of-1">
        {!v.container10}
    </div>

</aura:component>