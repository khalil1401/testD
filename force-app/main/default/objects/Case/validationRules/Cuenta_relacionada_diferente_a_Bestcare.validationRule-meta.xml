<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cuenta_relacionada_diferente_a_Bestcare</fullName>
    <active>false</active>
    <errorConditionFormula>AND (  RecordType.Name = &apos;Servicio&apos;, Account.RecordType.Name  &lt;&gt; &apos;Paciente Best Care&apos;)</errorConditionFormula>
    <errorMessage>No puede crear un caso de servicio para un paciente que no sea Bestcare.</errorMessage>
</ValidationRule>
