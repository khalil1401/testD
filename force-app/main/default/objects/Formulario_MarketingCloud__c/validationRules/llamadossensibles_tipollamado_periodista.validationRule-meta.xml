<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>llamadossensibles_tipollamado_periodista</fullName>
    <active>false</active>
    <description>En caso de haber seleccionado en &quot;Tipo de llamado&quot; la opcion &quot;Periodista&quot; se debe especificar el medio</description>
    <errorConditionFormula>AND( ISPICKVAL( Tipo_de_llamado__c , &quot;Periodista&quot;), ISBLANK(  Medio_de_comunicacion_al_que_pertenece__c  ) )</errorConditionFormula>
    <errorDisplayField>Medio_de_comunicacion_al_que_pertenece__c</errorDisplayField>
    <errorMessage>Completar</errorMessage>
</ValidationRule>
