<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Restricciones_de_fecha_en_carga</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
Fecha__c &lt; (TODAY () - 7),
$Profile.Name = &apos;Visitador médico&apos;
)</errorConditionFormula>
    <errorDisplayField>Fecha__c</errorDisplayField>
    <errorMessage>Se permite solo cargar visitas de hace máximo 7 días.</errorMessage>
</ValidationRule>
