<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Licencia_de_lunes_a_viernes</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISNEW(),
CASE(MOD( DATEVALUE(CreatedDate) - DATE(1900, 1, 7), 7),
0, 0,
6, 0,
1) = 0
)</errorConditionFormula>
    <errorMessage>Solo se pueden cargar licencias de lunes a viernes.</errorMessage>
</ValidationRule>
