<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Dan360_TiempoTranscurrido__c</fullName>
    <description>Campo que calcula el tiempo transcurrido desde que un pedido fue confirmado</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Status, &apos;Confirmado&apos;), (NOW() - Dan360_FechasConfirmado__c) *  24, Null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Tiempo Transcurrido Hrs.</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
