    <rules>
        <fullName>Aviso de caso de prioridad alta</fullName>
        <actions>
            <name>Caso_prioridad_alta</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND( RecordType.DeveloperName == &apos;Reclamo&apos; ,
ISPICKVAL(Subtipo__c, &apos;Calidad&apos;),
ISCHANGED(Priority),
ISPICKVAL(Priority, &apos;Alta&apos;),
NOT(ISPICKVAL(Status, &apos;Cerrado&apos;))
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
